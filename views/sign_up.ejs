<link rel="stylesheet" href="/css/sign_up.css">

<body>
    <div id="center">
        <div class="view-form">
            <div id="sign-up-form">
                <form action="/users/create" method="post">
                    <input type="text" name="name" placeholder="Enter your name" required>
                    <input type="email" name="email" placeholder="Enter your email" required>
                    <input type="password" name="password" placeholder="Entet your password" required>
                    <input type="password" name="confirm_password" placeholder="Enter your password Again" required>
                    <button type="submit" id="btn">Create Account</button>
                </form>
            </div>
        </div>
    </div>
</body>